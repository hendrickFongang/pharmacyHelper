<%- include('header'); -%>

<table class="table table-hover table-dark table1" style="margin-top:25px;">
  <thead>
    <tr>
      <th scope="col" style="color: yellow">#</th>
      <th scope="col" style="color: yellow"> <b> Date </b> </th>
      <th scope="col" style="color: yellow"> <b> Pharmacies </b> </th>
    </tr>
  </thead>
  <tbody>
    <% for(var i=0; i< docs.length; i++) { %>
      <tr>
        <th scope="row">  <%= i +1 %> </th>
        <th scope="row">  <%= docs[i].date %> </th>
        <th scope="row">  <%= docs[i].pharmacies %> </th>
      </tr>
    <% } %>
  </tbody>
</table>
<h6 style="color: red" > <em> <%= message3 %> </em> </h6>
